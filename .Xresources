Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
urxvt*cursorBlink: true

! URxvt*iconFile: /usr/share/icons/elFaenza/apps/48/Terminal.png

URxvt.geometry: 94x22

URxvt.scrollBar: false

! URxvt.font: xft:Tewi:regular:size=10
! URxvt.boldFont: xft:Tewi:regular:size=10
! URxvt.font: xft:Anonymice\ Powerline:regular:pixelsize=12
URxvt.font: xft:Inconsolata\ for\ Powerline:pixelsize=13,xft:Tewi:size=13:antialias=true:hinting=true

*background:#1a1a1a
*foreground:#fafafa
*highlightColor:#444444

! Black
*color0:#666666
*color8:#eeeeee

! White
*color7:#cccccc
*color15:#fafafa

! Red
*color1:#A7646A
*color9:#C69195

! Green
*color2:#637F53
*color10:#93A77F

! Yellow
*color3:#F4E477
*color11:#FBEFA1

! Blue
*color4:#648EA7
*color12:#91B6C6

! Purple
*color5:#555584
*color13:#7D7DA8

! Cyan
*color6:#64A794
*color14:#96C3B6

! Click URLs
URxvt.perl-ext-common:  default,matcher
URxvt.url-launcher:      /usr/bin/google-chrome
URxvt.matcher.button:   1

urxvt.iso14755:false
urxvt.iso10646:true
